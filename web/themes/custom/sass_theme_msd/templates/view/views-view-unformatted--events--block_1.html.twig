{% if title %}
<h3>{{ title }}</h3>
{% endif %}

<div class="make-carosel container events-view-block-wrapper">
  {% for row in rows %}
  {%
  set row_classes = [
  default_row_class ? 'views-row',
  ]
  %}
  <div {{ row.attributes.addClass(row_classes) }}>
    {{- row.content -}}
  </div>
  {% endfor %}
</div>