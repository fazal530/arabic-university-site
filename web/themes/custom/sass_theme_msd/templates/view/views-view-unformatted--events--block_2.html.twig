{% if title %}
  <h3>{{ title }}</h3>
{% endif %}

<div class="grid container events-node-view-wrapper">
  {% for row in rows %}
    {%
      set row_classes = [
        default_row_class ? 'views-row',
      ]
    %}
    <div class="events-node-view" {{ row.attributes.addClass(row_classes) }}>
      {{- row.content -}}
    </div>
  {% endfor %}
</div>
